{
  "name": "@mlcpro/webpack5config",
  "version": "1.0.0",
  "description": "Initializes a webpack 5 configuration for any new project",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack --config webpack.dev.js",
    "serve": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --progress",
    "watch": "webpack --config webpack.dev.js --watch",
    "version": "webpack --version",
    "help": "webpack --help=verbose"
  },
  "keywords": [
    "webpack",
    "config",
    "css",
    "scss",
    "sass",
    "assets"
  ],
  "author": "Mlc (https://github.com/mlcpro)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
